{
  "name": "@flayyer/use-googlefonts",
  "version": "0.3.0",
  "description": "React hook to dynamically load Google Fonts v2 during execution.",
  "keywords": [
    "flayyer",
    "react",
    "react-hook",
    "fonts",
    "googlefonts",
    "typescript"
  ],
  "main": "dist/use-googlefonts.es5.js",
  "module": "dist/use-googlefonts.esm.js",
  "typings": "dist/use-googlefonts.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "Patricio LÃ³pez Juri <patricio@flayyer.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/flayyer/use-googlefonts.git"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.ts",
    "prepublishOnly": "yarn run build",
    "start": "rollup -c rollup.config.ts -w",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "fontfaceobserver": "^2.1.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "devDependencies": {
    "@flayyer/eslint-config": "^1.0.6",
    "@testing-library/react-hooks": "^5.1.1",
    "@types/fontfaceobserver": "^0.0.6",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "eslint": "^7.23.0",
    "google-fonts-css2": "^1.0.3",
    "husky": "^4.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ]
  }
}
